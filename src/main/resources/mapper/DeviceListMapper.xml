<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cuit.homeiot.mapper.DeviceListMapper">

    <select id="countByTypeLong" resultType="java.lang.Long">
        SELECT COUNT(*)
        FROM Device_List as D
        WHERE D.type = #{type}
    </select>
</mapper>